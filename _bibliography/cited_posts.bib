	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/14680629.2023.2191734},
	keywords = {Heterogeneous simulation, bituminous mastic, fatigue behaviour, multiscale approach},
	pages = {1--12},
}

