	author = {Fernandez-Zelaia, Patxi and Yabansu, Yuksel C. and Kalidindi, Surya R.},
	month = jun,
	year = {2019},
	pages = {67--81},
}

@incollection{chaboche_continuum_1998,
	title = {Continuum damage mechanics of composites: {Towards} a unified approach},
