	keywords = {2-D SiC/SiC, Anisotropic damage, Ceramic matrix composites, Constitutive model, Continuum damage mechanics},
	pages = {919--942},
}

@article{callens_tensile_2015,
	title = {Tensile behaviour of stainless steel fibre/epoxy composites with modified adhesion},
