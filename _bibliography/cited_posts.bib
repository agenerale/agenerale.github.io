	keywords = {-dry woven fabric unit cells, Global sensitivity analysis, Latin hypercube sampling, Uncertainty criteria},
	pages = {1--17},
}

@article{el_moumen_numerical_2021,
	title = {Numerical evaluation of the representative volume element for random composites},
	volume = {86},
