@article{choudhury_quantification_2016,
	title = {Quantification and classification of microstructures in ternary eutectic alloys using 2-point spatial correlations and principal component analyses},
	volume = {110},
	issn = {1359-6454},
	url = {http://www.sciencedirect.com/science/article/pii/S1359645416301604},
	doi = {10.1016/j.actamat.2016.03.010},
	abstract = {Eutectic solidification gives rise to a number of distinct microstructure patterns that might include lamella, rods and labyrinths in binary alloys. However, as the number of phases and components increases, the number of possible patterns that might be obtained during bulk solidification also become larger. While the morphological attributes of binary eutectic solidification have been fairly well understood, the same is not true for ternary and higher multicomponent alloys. In this paper, we study and quantify microstructures in ternary alloys as a function of two essential parameters, namely, the volume fraction of the solid phases and the surface energies of the interfaces (in particular the solid–liquid interfaces). For the selected ensemble of microstructures, quantification and classification were carried out using a recently developed data-driven (objective) approach based on principal component analyses of 2-point correlations. It is demonstrated that the method is capable of analyzing and quantifying the similarity/difference measures between the elements of the selected ensemble of microstructures.},
	language = {en},
	urldate = {2020-05-18},
	journal = {Acta Materialia},
	author = {Choudhury, Abhik and Yabansu, Yuksel C. and Kalidindi, Surya R. and Dennstedt, Anne},
	month = may,
	year = {2016},
	keywords = {CALPHAD, PCA, Pattern-formation, Phase-field, Ternary eutectic},
	pages = {131--141},
}

@article{niezgoda_delineation_2008,
	title = {Delineation of the space of 2-point correlations in a composite material system},
	volume = {56},
	issn = {1359-6454},
	url = {http://www.sciencedirect.com/science/article/pii/S1359645408004886},
	doi = {10.1016/j.actamat.2008.07.005},
	abstract = {The complete set of 2-point correlations for a composite material system with a large number of local states (e.g. polycrystalline metals) forms a vast and unwieldy data set containing a large amount of redundant information. The interrelations in these correlations have been well characterized for composite material systems with two local states, but only a small number have been delineated for the composite systems with many local states. This paper presents an analysis of interrelations between the complete set of 2-point correlations for composite material systems through their spectral representations via discrete Fourier transforms. These interrelations are used to delineate a compact and convex space that bounds the set of all physically realizable 2-point correlations called the 2-point correlations hull. The representation of any given microstructure in this hull, and the techniques to produce a representative volume element are also explored in this paper.},
	language = {en},
	number = {18},
	urldate = {2020-05-18},
	journal = {Acta Materialia},
	author = {Niezgoda, S. R. and Fullwood, D. T. and Kalidindi, S. R.},
	month = oct,
	year = {2008},
	keywords = {2-Point correlation, Discrete Fourier transform, Microstructure, Pair correlation function, Statistics},
	pages = {5285--5292},
}

@article{adams_finite_2005,
	title = {Finite approximations to the second-order properties closure in single phase polycrystals},
	volume = {53},
	issn = {1359-6454},
	url = {http://www.sciencedirect.com/science/article/pii/S1359645405002235},
	doi = {10.1016/j.actamat.2005.03.052},
	abstract = {Extension of the first-order theory of microstructure design to considerations of morphological texture is addressed in this paper. The main challenges include the r-interdependence of the 2-point correlation functions of lattice orientation, construction of the corresponding microstructure hull, and its corresponding properties closure(s). It is shown that the correlation functions can be expressed in terms of an intermediate construct, called the texture function; the correlation functions have quadratic dependence in the texture functions. A complete (finite) texture hull is readily constructed for the texture functions in Fourier space, and is found to be a convex polytope. Eigen-texture functions occupy its corner (extreme) points. Microstructure design proceeds directly from homogenization relations evaluated at the corner points. This gives rise to (combined) properties closures, from which second-order microstructure design can proceed. This is demonstrated in a brief case study.},
	language = {en},
	number = {13},
	urldate = {2020-05-18},
	journal = {Acta Materialia},
	author = {Adams, Brent L. and Gao, Xiang (Carl) and Kalidindi, Surya R.},
	month = aug,
	year = {2005},
	keywords = {Mechanical properties-elastic behavior, Mesostructure, Microstructure-texture, Statistical mechanics},
	pages = {3563--3577},
}

@article{morscher_stress-environmental_2014,
	title = {Stress-{Environmental} {Effects} on {Fiber}-{Reinforced} {SiC}-{Based} {Composites}},
	doi = {10.1002/9781118832998.ch11},
	abstract = {Fiber-reinforced SiC-based composites are state-of-the-art enabling materials currently being used or developed for high temperature, extreme environment use in aerospace and energy applications. However, the performance of these composites is limited due to strength-degrading oxidation of the non-oxide composite constituents. The form of this degradation depends essentially on three factors: (1) the composite system itself, that is, the composite constituents (and to some extent the fiber architecture (structure) of a specific composite) dictate what reactions can occur with the environment; (2) the applied stress state, which dictates the micro-damage state often responsible for oxygen ingress into a specimen; and (3) the thermochemical environment, which contains the oxidizing species or other more corrosive species. The rates of oxidation, time-temperature damage progression, and fiber-strength degradation all depend on these three factors as well. This chapter will discuss the mechanisms causing stressed-oxidation degradation in SiC-based composites as well as the stress-oxidative behavior of specific composite systems. In addition, modeling and design approaches will be discussed as a means to establish life-prediction methodologies critical for ultimate use of these types of materials in critical applications.},
	journal = {Ceramic Matrix Composites: Materials, Modeling and Technology},
	author = {Morscher, Greg},
	month = oct,
	year = {2014},
	pages = {334--352},
}

