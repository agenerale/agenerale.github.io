	journal = {Computational Materials Science},
	author = {Fetni, Seifallah and Pham, Thinh Quy Duc and Hoang, Truong Vinh and Tran, Hoang Son and Duchêne, Laurent and Tran, Xuan-Van and Habraken, Anne Marie},
	month = jan,
