@article{oliver_consistent_1989,
	title = {A consistent characteristic length for smeared cracking models},
	volume = {28},
	issn = {0029-5981, 1097-0207},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/nme.1620280214},
	doi = {10.1002/nme.1620280214},
	abstract = {A numerical scheme for crack modelling by means of continuous displacement fields is presented. In twodimensional problems a crack is modelled as a limiting case of two singular lines (with continuous displaccnients, but discontinuous displacement gradients across them) which tend to coincide with each other. An analysis of the energy dissipated inside the band bounded by both lines allows one to obtain an expression for the characteristic length as the ratio between the energy dissipated per unit surface area (fracture energy) and the energy dissipated per unit volume (specific energy) at a point. The application of these mathematical expressions to the finite element discretized medium allow one to obtain a general spatial and directional expression for the characteristic length which guarantees the objectivity of the results with respect to the size of the finite element mesh. The numerical results presented show the reliability of the proposed expressions.},
	language = {en},
	number = {2},
	urldate = {2021-10-12},
	journal = {International Journal for Numerical Methods in Engineering},
	author = {Oliver, J.},
	month = feb,
	year = {1989},
	pages = {461--474},
}

