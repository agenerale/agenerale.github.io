	keywords = {Bayes methods, Computational modeling, Hidden Markov models, Market research, Optimization, Probabilistic logic, Stochastic processes, Variational inference, approximate Bayesian inference, inference networks, reparameterization gradients, scalable inference, structured variational approximations},
	pages = {2008--2026},
}

