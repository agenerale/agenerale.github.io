	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.594},
	keywords = {automated meshing, effective properties, homogenization, mesh refinement, periodic media, textile composites, unit-cell-analysis},
	pages = {977--1006},
}

