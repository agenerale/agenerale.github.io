@article{fernandez-zelaia_comparative_2019,
	title = {A {Comparative} {Study} of the {Efficacy} of {Local}/{Global} and {Parametric}/{Nonparametric} {Machine} {Learning} {Methods} for {Establishing} {Structure}–{Property} {Linkages} in {High}-{Contrast} {3D} {Elastic} {Composites}},
