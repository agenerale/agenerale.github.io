	author = {Fast, Tony and Knezevic, Marko and Kalidindi, Surya R.},
	month = aug,
	year = {2008},
	keywords = {61.50.Ah, 62.20.Dc, 62.20.Fe, 62.20.−x, Anisotropy, Elasticity, Microstructure, Plasticity, Property closures, Spectral methods},
	pages = {374--383},
}

